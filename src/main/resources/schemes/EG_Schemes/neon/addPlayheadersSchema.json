{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "title": {
        "type": "string"
      },
      "duration": {
        "type": "integer"
      },
      "playheads": {
        "type": "string"
      },
      "image": {
        "type": "string",
        "format": "uri"
      },
      "mediaType": {
        "type": "string",
        "enum": [
          "episode"
        ]
      },
      "season": {
        "type": "integer"
      },
      "episode": {
        "type": "integer"
      }
    },
    "required": [
      "id",
      "title",
      "duration",
      "playheads",
      "image",
      "mediaType",
      "season",
      "episode"
    ]
  }
}
