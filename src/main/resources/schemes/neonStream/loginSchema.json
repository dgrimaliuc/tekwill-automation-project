{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "kind": {
      "type": "string",
      "enum": ["identitytoolkit#VerifyPasswordResponse"]
    },
    "localId": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "displayName": {
      "type": "string",
      "maxLength": 255
    },
    "idToken": {
      "type": "string"
    },
    "registered": {
      "type": "boolean"
    }
  },
  "required": [
    "kind",
    "localId",
    "email",
    "idToken",
    "registered"
  ]
}